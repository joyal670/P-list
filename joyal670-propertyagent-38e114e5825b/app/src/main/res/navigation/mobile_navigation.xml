<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.property.propertyagent.agent_panel.ui.main.home.home.HomeFragment"
        android:label="@string/nill"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_request"
        android:name="com.property.propertyagent.agent_panel.ui.main.home.request.RequestFragment"
        android:label="@string/my_request"
        tools:layout="@layout/fragment_request" />

    <fragment
        android:id="@+id/navigation_client"
        android:name="com.property.propertyagent.agent_panel.ui.main.home.myclient.MyClientFragment"
        android:label="@string/my_client"
        tools:layout="@layout/fragment_myclient" />

    <fragment
        android:id="@+id/navigation_mytask"
        android:name="com.property.propertyagent.agent_panel.ui.main.home.mytask.MyTaskFragment"
