                        android:layout_height="@dimen/margin_50dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:backgroundTint="@color/color_accent_green"
                        android:fontFamily="@font/heebo_medium"
                        android:text="@string/view_profile"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_11sp"
                        app:cornerRadius="@dimen/margin_4dp"
                        app:layout_constraintBottom_toBottomOf="@id/agentVisitorImage"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/agentVisitorImage" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp1"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/margin_15dp"
                    android:layout_marginEnd="@dimen/margin_15dp"
                    android:background="@color/color_accent_grey_light_new2" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_10dp"
                    android:layout_marginTop="@dimen/margin_15dp"
                    android:drawablePadding="@dimen/margin_10dp"
                    android:fontFamily="@font/heebo_bold"
                    android:text="@string/visit_status"
                    android:textColor="@color/color_accent_grey"
                    android:textSize="@dimen/text_size_13sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_5dp"
                    android:layout_marginTop="@dimen/margin_10dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/ivAgentStatusCode1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_group_ellipse_green"
                            tools:ignore="ContentDescription" />

                        <View
                            android:id="@+id/viewLine1"
                            android:layout_width="2dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/dotted_line"
                            android:layerType="software"
                            android:minHeight="?attr/actionBarSize" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginBottom="@dimen/margin_8dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tvAgentStatusVisited1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_medium"
                            android:paddingStart="@dimen/margin_5dp"
                            android:paddingEnd="@dimen/margin_5dp"
                            android:paddingBottom="@dimen/margin_5dp"
                            android:text="@string/visit_started"
                            android:textColor="@color/color_accent_green"
                            android:textSize="@dimen/text_size_15sp" />

                        <TextView
                            android:id="@+id/tvAgentStatusPropertyVisited1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_medium"
                            android:padding="@dimen/margin_5dp"
                            android:text="@string/property_visited"
                            android:textColor="@color/color_accent_grey"
                            android:textSize="@dimen/text_size_13sp" />

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_5dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/ivAgentStatusCode2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_ellipse_yellow"
                            tools:ignore="ContentDescription" />

                        <View
                            android:id="@+id/viewLine2"
                            android:layout_width="2dp"
                            android:layout_height="wrap_content"
                            android:background="@drawable/dotted_line"
                            android:layerType="software"
                            android:minHeight="?attr/actionBarSize" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginBottom="@dimen/margin_8dp"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tvAgentStatusVisited2"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_medium"
                            android:paddingStart="@dimen/margin_5dp"
                            android:paddingEnd="@dimen/margin_5dp"
                            android:paddingBottom="@dimen/margin_5dp"
                            android:text="@string/proceed_to_book"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_15sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="@dimen/margin_5dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_medium"
