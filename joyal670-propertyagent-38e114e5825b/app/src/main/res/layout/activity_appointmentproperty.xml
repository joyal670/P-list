<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <include
            layout="@layout/toolbar_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="?actionBarSize">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/margin_5dp"
                app:layout_constraintTop_toTopOf="parent">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_5dp"
                    android:orientation="horizontal"
                    android:weightSum="5">

                    <com.property.propertyagent.utils.RoundRectCornerImageView
                        android:id="@+id/agentPropertyImage"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_gravity="top"
                        android:layout_marginStart="@dimen/margin_10dp"
                        android:src="@drawable/sample_apartment" />

                    <LinearLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_16dp"
                        android:layout_weight="4.8"
                        android:orientation="vertical">

                        <TextView
                            android:id="@+id/tvAgentPropertyName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_medium"
                            android:text="@string/property_name"
                            android:textColor="@color/color_accent_grey_manrope"
                            android:textSize="@dimen/text_size_13sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_bold"
                                android:text="@string/property_code"
                                android:textColor="@color/color_accent_grey"
                                android:textSize="@dimen/text_size_13sp" />

                            <TextView
                                android:id="@+id/agentPropertyCode"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_bold"
                                android:text=""
                                android:textColor="@color/color_accent_grey"
                                android:textSize="@dimen/text_size_13sp" />

                        </LinearLayout>

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/margin_4dp"
                            android:orientation="horizontal">

                            <!--        <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:fontFamily="@font/heebo_bold"
                                        android:text="@string/RentalProperty"
                                        android:textColor="@color/color_accent_grey"
                                        android:textSize="@dimen/text_size_13sp" />

                                    <TextView
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="@dimen/margin_5dp"
                                        android:layout_marginEnd="@dimen/margin_5dp"
                                        android:fontFamily="@font/heebo_bold"
                                        android:text="@string/full_collen"
                                        android:textColor="@color/color_accent_grey"
                                        android:textSize="@dimen/text_size_13sp" />-->

                            <TextView
                                android:id="@+id/agentPropertyTo"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_medium"
                                android:text=""
                                android:textColor="@color/color_accent_grey_manrope"
                                android:textSize="@dimen/text_size_14sp" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_10dp"
                    android:orientation="horizontal">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/property_apponiment_viewPropertyDetails"
                        style="@style/material_button"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/margin_50dp"
                        android:layout_marginStart="@dimen/margin_10dp"
                        android:backgroundTint="@color/color_accent_green"
                        android:ellipsize="end"
                        android:fontFamily="@font/heebo_medium"
                        android:gravity="center"
                        android:maxLines="1"
                        android:text="@string/view_property_details"
                        android:textAllCaps="false"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintStart_toEndOf="@+id/property_apponiment_viewPackages"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/property_apponiment_viewPackages"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/margin_50dp"
                        android:layout_gravity="end"
                        android:layout_marginEnd="@dimen/margin_14dp"
                        android:autoSizeTextType="none"
                        android:backgroundTint="@color/color_accent_green"
                        android:ellipsize="end"
                        android:fontFamily="@font/heebo_medium"
                        android:text="@string/view_packages"
                        android:textAllCaps="false"
                        android:textSize="@dimen/text_size_11sp"
                        android:visibility="gone"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintHorizontal_bias="0.5"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:targetApi="o" />

                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/appoinmentfrg_prcing"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_10dp"
                    android:layout_marginTop="@dimen/margin_10dp"
                    android:drawablePadding="@dimen/margin_10dp"
                    android:fontFamily="@font/heebo_medium"
                    android:text="@string/Pricing"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_13sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_10dp"
                    android:layout_marginTop="@dimen/margin_7dp"
                    android:orientation="horizontal"
                    android:weightSum="6">

                    <TextView
                        android:id="@+id/txtAmountHead"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.75"
                        android:fontFamily="@font/heebo_bold"
                        android:text="@string/Rent"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:layout_weight=".5"
                        android:fontFamily="@font/heebo_bold"
                        android:text="@string/full_collen"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                    <TextView
                        android:id="@+id/agentRentAmount"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.75"
                        android:fontFamily="@font/heebo_medium"
                        android:text=""
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutSecurity"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_10dp"
                    android:layout_marginTop="@dimen/margin_4dp"
                    android:orientation="horizontal"
                    android:weightSum="6">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.75"
                        android:fontFamily="@font/heebo_bold"
                        android:text="@string/SecurityDeposit"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:layout_weight=".5"
                        android:fontFamily="@font/heebo_bold"
                        android:text="@string/full_collen"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                    <TextView
                        android:id="@+id/agentSecurityDeposit"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.75"
                        android:fontFamily="@font/heebo_medium"
                        android:text=""
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                </LinearLayout>

                <LinearLayout
