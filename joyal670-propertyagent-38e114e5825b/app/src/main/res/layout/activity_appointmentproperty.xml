                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_15sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="@dimen/margin_5dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_medium"
                                android:text="@string/Status"
                                android:textColor="@color/color_accent_yellow"
                                android:textSize="@dimen/text_size_13sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/margin_5dp"
                                android:layout_marginEnd="@dimen/margin_5dp"
                                android:fontFamily="@font/heebo_medium"
                                android:text="@string/full_collen"
                                android:textColor="@color/color_accent_yellow"
                                android:textSize="@dimen/text_size_15sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_medium"
                                android:text="@string/pending"
                                android:textColor="@color/color_accent_yellow"
                                android:textSize="@dimen/text_size_13sp" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_5dp"
                    android:orientation="horizontal">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:gravity="center_horizontal"
                        android:orientation="vertical">

                        <ImageView
                            android:id="@+id/ivAgentStatusCode5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/ic_ellipse_yellow"
                            tools:ignore="ContentDescription" />

                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:orientation="vertical"
                        tools:ignore="TooManyViews">

                        <TextView
                            android:id="@+id/tvAgentStatusVisited5"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_medium"
                            android:paddingStart="@dimen/margin_5dp"
                            android:paddingEnd="@dimen/margin_5dp"
                            android:paddingBottom="@dimen/margin_5dp"
                            android:text="@string/full_payment_key_handover"
                            android:textColor="@color/black"
                            android:textSize="@dimen/text_size_15sp" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal"
                            android:padding="@dimen/margin_5dp">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_medium"
                                android:text="@string/Status"
                                android:textColor="@color/color_accent_yellow"
                                android:textSize="@dimen/text_size_13sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/margin_5dp"
                                android:layout_marginEnd="@dimen/margin_5dp"
                                android:fontFamily="@font/heebo_medium"
                                android:text="@string/full_collen"
                                android:textColor="@color/color_accent_yellow"
                                android:textSize="@dimen/text_size_15sp" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/heebo_medium"
                                android:text="@string/pending"
                                android:textColor="@color/color_accent_yellow"
                                android:textSize="@dimen/text_size_13sp" />

                        </LinearLayout>

                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/layoutCommission"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/margin_5dp"
                    android:gravity="center_vertical"
                    android:orientation="horizontal">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/heebo_bold"
                        android:text="@string/add_commission"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/btnAddCommision"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="@dimen/margin_40dp"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:drawableStart="@drawable/ic_plus"
                        android:drawableTint="@color/color_accent_grey"
                        android:fontFamily="@font/heebo_medium"
                        android:text="@string/add"
                        android:textAllCaps="false"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_11sp"
                        app:cornerRadius="@dimen/margin_5dp"
                        app:strokeColor="@color/color_accent_grey" />

                    <TextView
                        android:id="@+id/tvCommission"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/heebo_bold"
                        android:text=""
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp"
                        android:visibility="gone" />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/btnEditCommision"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="@dimen/margin_60dp"
                        android:layout_height="@dimen/margin_40dp"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:fontFamily="@font/heebo_medium"
                        android:text="@string/edit"
                        android:textAllCaps="false"
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_11sp"
                        android:visibility="gone"
                        app:cornerRadius="@dimen/margin_5dp"
                        app:strokeColor="@color/color_accent_grey" />

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/margin_25dp"
                    android:layout_marginBottom="@dimen/margin_10dp"
                    android:orientation="horizontal"
                    android:weightSum="2">

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/btnNotInterested"
                        style="@style/Widget.MaterialComponents.Button.OutlinedButton"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/margin_50dp"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/heebo_medium"
                        android:text="@string/not_interested"
                        android:textAllCaps="false"
                        android:textColor="@color/color_accent_orange"
                        android:textSize="@dimen/text_size_11sp"
                        app:cornerRadius="@dimen/margin_5dp"
                        app:strokeColor="@color/color_accent_orange" />

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/appoinmentfrg_porcedtobookBtn"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/margin_50dp"
                        android:layout_marginStart="@dimen/margin_5dp"
                        android:layout_marginEnd="@dimen/margin_5dp"
                        android:layout_weight="1"
                        android:backgroundTint="@color/color_accent_green"
                        android:fontFamily="@font/heebo_medium"
                        android:text="@string/proceed_to_book"
                        android:textAllCaps="false"
                        android:textColor="@color/white"
                        android:textSize="@dimen/text_size_11sp"
                        app:cornerRadius="@dimen/margin_5dp" />

                </LinearLayout>

            </LinearLayout>

        </androidx.core.widget.NestedScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>