                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="2.75"
                        android:fontFamily="@font/heebo_medium"
                        android:text=""
                        android:textColor="@color/color_accent_grey"
                        android:textSize="@dimen/text_size_13sp" />

                </LinearLayout>

                <View
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/dp1"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/margin_15dp"
                    android:layout_marginTop="@dimen/margin_25dp"
                    android:layout_marginEnd="@dimen/margin_15dp"
                    android:background="@color/color_accent_grey_light_new2" />

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:padding="@dimen/margin_10dp"
                    android:weightSum="3">

                    <ImageView
                        android:id="@+id/agentVisitorImage"
                        android:layout_width="32dp"
                        android:layout_height="@dimen/margin_45dp"
                        android:layout_gravity="center"
                        android:padding="@dimen/margin_5dp"
                        android:src="@drawable/ic_color_line_user"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        tools:ignore="ContentDescription" />

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/margin_14dp"
                        android:layout_marginTop="@dimen/margin_3dp"
                        android:orientation="vertical"
                        app:layout_constraintBottom_toBottomOf="@id/agentVisitorImage"
                        app:layout_constraintStart_toEndOf="@id/agentVisitorImage"
                        app:layout_constraintTop_toTopOf="@id/agentVisitorImage">

                        <TextView
                            android:id="@+id/agentVisitorName"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_bold"
                            android:text=""
                            android:textColor="@color/color_accent_grey"
                            android:textSize="@dimen/text_size_13sp" />

                        <TextView
                            android:id="@+id/agentVisitoryEmail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/heebo_medium"
                            android:text=""
                            android:textColor="@color/color_accent_grey_manrope"
                            android:textSize="@dimen/text_size_11sp" />

                    </LinearLayout>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/property_apponiment_viewProfileTv"
