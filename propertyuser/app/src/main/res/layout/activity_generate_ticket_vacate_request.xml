<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.main.my_property.view_details.generate_ticket_vacate_request.GenerateTicketVacateRequestActivity">

    <include
        layout="@layout/toolbar_main"
        android:id="@+id/include"/>
    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:id="@+id/nested"
        android:fillViewport="true"
        app:layout_constraintTop_toBottomOf="@+id/include"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearMain"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubOne"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvTicketTypeText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketTypeText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvTicketType"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketType"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine1"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubTwo"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvTicketRaiseOnText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketRaiseOnText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvTicketRaiseOnDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketRaiseOnDate"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine2"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubThree"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvPropertyIdText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyIdText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvPropertyId"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyId"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine3"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubFour"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvContractStartDateText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvContractStartDateText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvContractStartDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvContractStartDate"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine4"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubFive"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvContractEndDateText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvContractEndDateText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvContractEndDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvContractEndDate"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine5"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubSix"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvStatusText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvStatusText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvStatus"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvStatus1"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine6"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubSeven"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvPropertyVerificationText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyVerificationText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvPropertyVerification"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyVerification"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine7"
                android:visibility="gone"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubEight"
                android:orientation="horizontal"
                android:visibility="gone"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvPropertyDamageText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyDamageText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvPropertyDamageDeduction"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/sar_0_0"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine8"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubNine"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_30dp">

                <TextView
                    android:id="@+id/tvDueAmountText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvDueAmountText"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_14sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvDueAmount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvDueAmount1"
                    android:textColor="@color/black"
                    android:textSize="@dimen/text_size_16sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubTen"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvSecurityDepositText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvSecurityDepositText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvSecurityDepositAmount"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvSecurityDepositAmount"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>

            <TextView
                android:id="@+id/tvTicketClosed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:fontFamily="@font/manrope_semibold"
                android:maxLines="1"
                android:text="@string/tvTicketClosed"
                android:textColor="@color/green_200"
                android:textSize="@dimen/text_size_14sp"
                android:textAlignment="center"
                android:background="@drawable/border_rounded_rect_lightgreen"
                android:layout_marginStart="@dimen/margin_20dp"
                android:layout_marginEnd="@dimen/margin_20dp"
                android:padding="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_40dp"
                android:layout_marginBottom="@dimen/margin_20dp"
                />
        </LinearLayout>

    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>