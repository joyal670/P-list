<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:fillViewport="true"
    android:id="@+id/nestedGeneratedTicketRequestedProperty">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearMainRequestPropertyTicket"
            android:orientation="vertical"
            android:visibility="gone">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubOne"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvTicketTypeText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketTypeText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvTicketTypeRequested"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:textDirection="locale"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketTypeRequested"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine1"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubTwo"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvTicketRaiseOnText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketRaiseOnText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvTicketRaiseOnDate"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvTicketRaiseOnDate"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine2"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubThree"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvDesiredPropertyTypeText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvDesiredPropertyTypeText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvDesiredPropertyType"
                    android:layout_width="wrap_content"
                    android:textDirection="locale"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvDesiredPropertyType"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine3"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubFour"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvPropertyToText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyToText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvPropertyTo"
                    android:layout_width="wrap_content"
                    android:textDirection="locale"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPropertyTo"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine4"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/linearSubFive"
                android:orientation="horizontal"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                android:layout_marginTop="@dimen/margin_16dp">

                <TextView
                    android:id="@+id/tvPreferedLocationText"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPreferedLocationText"
                    android:textColor="@color/grey1"
                    android:textSize="@dimen/text_size_12sp"
                    android:layout_gravity="center_vertical"

                    />
                <TextView
                    android:id="@+id/tvPreferedLocation"
                    android:layout_width="wrap_content"
                    android:textDirection="locale"
                    android:layout_height="wrap_content"
                    android:ellipsize="end"
                    android:fontFamily="@font/manrope_medium"
                    android:maxLines="1"
                    android:text="@string/tvPreferedLocation"
                    android:textColor="@color/black_light1"
                    android:textSize="@dimen/text_size_14sp"
                    android:textAlignment="textEnd"
                    android:layout_weight="1"
                    />
            </LinearLayout>
            <ImageView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/ivLine5"
                android:src="@drawable/ic_line_full"
                android:layout_marginTop="@dimen/margin_10dp"
                android:layout_marginStart="@dimen/margin_16dp"
                android:layout_marginEnd="@dimen/margin_16dp"
                />

            <androidx.recyclerview.widget.RecyclerView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:id="@+id/rvRequestPropertyGeneratedTicketList"
                android:layout_marginTop="@dimen/margin_25dp"
                tools:itemCount="1"
                tools:listitem="@layout/list_requested_property_generated_ticket_item"
                android:layout_marginBottom="@dimen/margin_10dp"/>
            <TextView
                android:id="@+id/tvTicketClosed"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:ellipsize="end"
                android:fontFamily="@font/manrope_semibold"
                android:maxLines="1"
                android:text="@string/tvTicketClosed"
                android:textColor="@color/green_200"
                android:textSize="@dimen/text_size_14sp"
                android:textAlignment="center"
                android:background="@drawable/border_rounded_rect_lightgreen"
                android:layout_marginStart="@dimen/margin_20dp"
                android:layout_marginEnd="@dimen/margin_20dp"
                android:padding="@dimen/margin_20dp"
                android:layout_marginTop="@dimen/margin_40dp"
                android:layout_marginBottom="@dimen/margin_20dp"
                android:visibility="gone"
                />
        </LinearLayout>
        <include
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/includeNoInternet"
            layout="@layout/layout_no_network"
            android:visibility="gone"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:id="@+id/linearNoDataFound"
            android:orientation="vertical"
            android:background="@color/white"
            android:visibility="gone">
            <include
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/includeNoDataFound"
                layout="@layout/layout_no_data_found"
                />
        </LinearLayout>
    </LinearLayout>
</androidx.core.widget.NestedScrollView>